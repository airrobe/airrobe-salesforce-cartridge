{
  "name": "int_airrobe_core",
  "version": "23.0.0",
  "description": "New overlay cartridge",
  "main": "index.js",
  "scripts": {
    "lint": "sgmf-scripts --lint js && sgmf-scripts --lint css",
    "lint:fix": "sgmf-scripts --lint js --fix && sgmf-scripts --lint css --fix",
    "upload": "sgmf-scripts --upload -- ",
    "uploadCartridge": "sgmf-scripts --uploadCartridge int_airrobe_core",
    "compile:js": "sgmf-scripts --compile js",
    "compile:scss": "sgmf-scripts --compile css",
    "test": "sgmf-scripts --test test/unit/**/*.js && npx mocha test/unit/**/getCategory.js --reporter spec && npx mocha test/unit/**/airrobeMultiOptInHelpers.js --reporter spec"
